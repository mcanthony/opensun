{
  "version": "0.2",
  "name": "ShotClock",
  "description": "Sun angle calculator for Outdoor Photographers",
  "launch_path": "/opensun",
  "icons": {
    "64": "/opensun/img/shotclock-logo-64.png",
    "128": "/opensun/img/shotclock-logo-128.png"
  },
  "developer": {
    "name": "Bill Walker",
    "url": "http://wfwalker.github.com/opensun/"
  },
  "installs_allowed_from": ["*"],
  "locales": {
  },
  "default_locale": "en",
  "permissions": {
    "geolocation": {
      "description": "Used to position the map to your current position"
    },
    "desktop-notification": {
      "description": "Used to tell the user when the sunlight is good"
    }    
  }  
}
